{
	"name": "df_chicagoCrimes",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_raw_csv_crime",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_temporal",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "DayOfWeekCount"
				}
			],
			"scriptLines": [
				"source(output(",
				"          id as integer,",
				"          case_number as string,",
				"          date as string,",
				"          block as string,",
				"          iucr as string,",
				"          primary_type as string,",
				"          description as string,",
				"          location_description as string,",
				"          arrest as boolean,",
				"          domestic as boolean,",
				"          beat as short,",
				"          district as short,",
				"          ward as short,",
				"          community_area as short,",
				"          fbi_code as string,",
				"          x_coordinate as integer,",
				"          y_coordinate as integer,",
				"          year as short,",
				"          updated_on as string,",
				"          latitude as double,",
				"          longitude as double,",
				"          {:@computed_region_awaf_s7ux} as short,",
				"          {:@computed_region_6mkv_f3dw} as short,",
				"          {:@computed_region_vrxf_vc4k} as short,",
				"          {:@computed_region_bdys_3d7i} as short,",
				"          {:@computed_region_43wa_7qmu} as short,",
				"          {:@computed_region_rpca_8um6} as short,",
				"          {:@computed_region_d9mm_jgwp} as short,",
				"          {:@computed_region_d3ds_rm58} as short",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 aggregate(groupBy(dateofweek = dayOfWeek(toDate(date,'yyyy-mm-dd'))),",
				"     iucr = count(iucr)) ~> DayOfWeekCount",
				"DayOfWeekCount sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}